{
    "name": "babdev/babdev.com",
    "description": "The babdev.com website",
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "path",
            "url": "nova-components/TinyMCEField"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-hash": "*",
        "composer-runtime-api": "^2.0",
        "babdev/laravel-breadcrumbs": "^1.4",
        "babdev/laravel-server-push-manager": "^2.1",
        "babdev/tinymce-field": "*",
        "blade-ui-kit/blade-ui-kit": "^0.3.3",
        "elhebert/laravel-sri": "^3.1",
        "guzzlehttp/guzzle": "^7.4.2",
        "http-interop/http-factory-guzzle": "^1.2",
        "knplabs/github-api": "^3.6",
        "laravel/framework": "^9.14",
        "laravel/nova": "^4.6.1",
        "lcobucci/jwt": "^4.1.5",
        "league/commonmark": "^2.3",
        "spatie/laravel-google-fonts": "^1.2",
        "spatie/laravel-sitemap": "^6.1",
        "spatie/laravel-sluggable": "^3.4",
        "spatie/packagist-api": "^2.0.2"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12.3",
        "fakerphp/faker": "^1.19",
        "laravel/telescope": "^4.9",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^6.2",
        "nunomaduro/larastan": "2.1.6",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "1.7.8",
        "phpstan/phpstan-mockery": "1.1.0",
        "phpstan/phpstan-phpunit": "1.1.1",
        "phpunit/phpunit": "^9.5.20",
        "spatie/laravel-ignition": "^1.2.3"
    },
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "BabDev\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ]
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        },
        "optimize-autoloader": true,
        "platform": {
            "php": "8.1.4"
        },
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
