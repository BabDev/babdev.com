{
    "name": "babdev/babdev.com",
    "description": "The babdev.com website",
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "path",
            "url": "nova-components/TinyMCEField"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-hash": "*",
        "composer-runtime-api": "^2.0",
        "babdev/laravel-breadcrumbs": "1.4.0",
        "babdev/laravel-server-push-manager": "2.1.0",
        "babdev/tinymce-field": "*",
        "blade-ui-kit/blade-ui-kit": "0.3.4",
        "elhebert/laravel-sri": "3.1.0",
        "guzzlehttp/guzzle": "7.5.0",
        "http-interop/http-factory-guzzle": "1.2.0",
        "knplabs/github-api": "3.8.0",
        "laravel/framework": "9.34.0",
        "laravel/nova": "4.15.2",
        "lcobucci/jwt": "4.2.1",
        "league/commonmark": "2.3.5",
        "spatie/laravel-google-fonts": "1.2.1",
        "spatie/laravel-sitemap": "6.2.2",
        "spatie/laravel-sluggable": "3.4.0",
        "spatie/packagist-api": "2.1.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "2.12.3",
        "fakerphp/faker": "1.20.0",
        "laravel/telescope": "4.9.4",
        "mockery/mockery": "1.5.1",
        "nunomaduro/collision": "6.3.1",
        "nunomaduro/larastan": "2.2.1",
        "phpstan/extension-installer": "1.1.0",
        "phpstan/phpstan": "1.8.8",
        "phpstan/phpstan-mockery": "1.1.0",
        "phpstan/phpstan-phpunit": "1.1.1",
        "phpunit/phpunit": "9.5.25",
        "spatie/laravel-ignition": "1.5.1"
    },
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "BabDev\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ]
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        },
        "optimize-autoloader": true,
        "platform": {
            "php": "8.1.7"
        },
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
