{
    "name": "babdev/babdev.com",
    "description": "The babdev.com website",
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-hash": "*",
        "composer-runtime-api": "^2.0",
        "babdev/laravel-breadcrumbs": "^1.3",
        "babdev/laravel-server-push-manager": "^2.0",
        "blade-ui-kit/blade-ui-kit": "^0.2",
        "elhebert/laravel-sri": "^3.0.1",
        "guzzlehttp/guzzle": "^7.3",
        "http-interop/http-factory-guzzle": "^1.2",
        "knplabs/github-api": "^3.3",
        "kraftbit/nova-tinymce5-editor": "^1.0",
        "laravel/framework": "^8.51",
        "laravel/nova": "^3.27",
        "lcobucci/jwt": "^4.1.4",
        "league/commonmark": "^1.6.6",
        "spatie/laravel-sitemap": "^6.0.4",
        "spatie/laravel-sluggable": "^3.1",
        "spatie/packagist-api": "^2.0.2"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.10",
        "facade/ignition": "^2.11.2",
        "fakerphp/faker": "^1.15",
        "laravel/telescope": "^4.6",
        "mockery/mockery": "^1.4.3",
        "nunomaduro/collision": "^5.5",
        "nunomaduro/larastan": "^0.7.11",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^0.12.93",
        "phpstan/phpstan-mockery": "^0.12.14",
        "phpstan/phpstan-phpunit": "^0.12.21",
        "phpunit/phpunit": "^9.5.7"
    },
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "BabDev\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.0.8"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
