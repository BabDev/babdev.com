{
    "name": "babdev/babdev.com",
    "description": "The babdev.com website",
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "path",
            "url": "./nova-components/NovaCKEditor4Field"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-hash": "*",
        "babdev/laravel-breadcrumbs": "^1.1",
        "babdev/laravel-server-push-manager": "^1.4",
        "babdev/nova-ckeditor4-field": "*",
        "barryvdh/laravel-elfinder": "^0.4.5",
        "drobee/nova-sluggable": "^1.2",
        "erusev/parsedown-extra": "^0.8.1",
        "guzzlehttp/guzzle": "^6.5.5",
        "knplabs/github-api": "^2.15",
        "laravel/framework": "^7.25",
        "laravel/nova": "^3.8.2",
        "lcobucci/jwt": "^3.3.2",
        "llaski/nova-scheduled-jobs": "^3.1",
        "php-http/guzzle6-adapter": "^2.0.1",
        "spatie/laravel-sitemap": "^5.7",
        "spatie/laravel-sluggable": "^2.5",
        "spatie/packagist-api": "^2.0",
        "symfony/dom-crawler": "^5.1.3"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.8",
        "facade/ignition": "^2.3.6",
        "friendsofphp/php-cs-fixer": "^2.16.4",
        "laravel/telescope": "^3.5.1",
        "nunomaduro/collision": "^4.2"
    },
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*"
    },
    "autoload": {
        "classmap": [
            "database/seeds"
        ],
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "BabDev\\": "app/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4.7"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
