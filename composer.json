{
    "name": "babdev/babdev.com",
    "description": "The babdev.com website",
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "path",
            "url": "./nova-components/NovaCKEditor4Field"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-hash": "*",
        "composer-runtime-api": "^2.0",
        "babdev/laravel-breadcrumbs": "^1.2",
        "babdev/laravel-server-push-manager": "^1.5",
        "babdev/nova-ckeditor4-field": "*",
        "barryvdh/laravel-elfinder": "^0.4.6",
        "blade-ui-kit/blade-ui-kit": "^0.1.1",
        "guzzlehttp/guzzle": "^7.2",
        "knplabs/github-api": "^2.15",
        "laravel/framework": "^8.10",
        "laravel/nova": "^3.12.1",
        "lcobucci/jwt": "^3.3.3",
        "league/commonmark": "^1.5.5",
        "php-http/guzzle7-adapter": "^0.1",
        "spatie/laravel-sitemap": "^5.8",
        "spatie/laravel-sluggable": "^2.5.2",
        "spatie/packagist-api": "^2.0.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.8.1",
        "facade/ignition": "^2.3.8",
        "friendsofphp/php-cs-fixer": "^2.16.4",
        "fzaninotto/faker": "^1.9.1",
        "laravel/telescope": "^4.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0.2",
        "phpunit/phpunit": "^9.4.1"
    },
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "BabDev\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4.9"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
