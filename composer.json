{
    "name": "babdev/babdev.com",
    "description": "The babdev.com website",
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        }
    ],
    "require": {
        "php": ">=7.2",
        "babdev/laravel-server-push-manager": "dev-master",
        "davejamesmiller/laravel-breadcrumbs": "^5.2.1",
        "laravel/framework": "5.8.*",
        "laravel/nova": "*",
        "spatie/laravel-sluggable": "^2.1.7",
        "waynestate/nova-ckeditor4-field": "^0.3.0"
    },
    "require-dev": {
        "filp/whoops": "^2.3.1",
        "friendsofphp/php-cs-fixer": "^2.14.2",
        "laravel/telescope": "^2.0.4",
        "nunomaduro/collision": "^3.0.1"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "BabDev\\": "app/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
