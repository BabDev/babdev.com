{
    "name": "babdev/babdev.com",
    "description": "The babdev.com website",
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "path",
            "url": "nova-components/TinyMCEField"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-hash": "*",
        "composer-runtime-api": "^2.0",
        "babdev/laravel-breadcrumbs": "2.0.0",
        "babdev/laravel-server-push-manager": "2.2.0",
        "babdev/tinymce-field": "*",
        "blade-ui-kit/blade-ui-kit": "0.4.0",
        "elhebert/laravel-sri": "dev-master",
        "guzzlehttp/guzzle": "7.5.0",
        "http-interop/http-factory-guzzle": "1.2.0",
        "knplabs/github-api": "3.9.0",
        "laravel/framework": "10.1.5",
        "laravel/nova": "4.22.1",
        "lcobucci/jwt": "4.3.0",
        "league/commonmark": "2.3.9",
        "spatie/laravel-feed": "4.2.1",
        "spatie/laravel-google-fonts": "1.2.3",
        "spatie/laravel-sitemap": "6.2.5",
        "spatie/laravel-sluggable": "3.4.2",
        "spatie/packagist-api": "2.1.0"
    },
    "require-dev": {
        "driftingly/rector-laravel": "0.15.0",
        "fakerphp/faker": "1.21.0",
        "laravel/telescope": "4.14.0",
        "mockery/mockery": "1.5.1",
        "nunomaduro/collision": "7.0.5",
        "nunomaduro/larastan": "2.4.1",
        "phpstan/extension-installer": "1.2.0",
        "phpstan/phpstan": "1.10.2",
        "phpstan/phpstan-mockery": "1.1.1",
        "phpstan/phpstan-phpunit": "1.3.7",
        "phpunit/phpunit": "10.0.12",
        "rector/rector": "0.15.18",
        "spatie/laravel-ignition": "2.0.0"
    },
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "BabDev\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ]
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": false,
            "phpstan/extension-installer": true
        },
        "optimize-autoloader": true,
        "platform": {
            "php": "8.2.3"
        },
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
