{
    "name": "babdev/babdev.com",
    "description": "The babdev.com website",
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "path",
            "url": "./nova-components/NovaCKEditor4Field"
        }
    ],
    "require": {
        "php": "^7.4",
        "babdev/laravel-server-push-manager": "^1.4",
        "babdev/nova-ckeditor4-field": "*",
        "barryvdh/laravel-elfinder": "^0.4.5",
        "davejamesmiller/laravel-breadcrumbs": "^5.3.2",
        "drobee/nova-sluggable": "^1.2",
        "ebess/advanced-nova-media-library": "^3.1",
        "guzzlehttp/guzzle": "^6.5.2",
        "knplabs/github-api": "^2.13",
        "laravel/framework": "^7.6.2",
        "laravel/nova": "^3.4",
        "michielkempen/nova-order-field": "^2.0.4",
        "php-http/guzzle6-adapter": "^2.0.1",
        "spatie/laravel-medialibrary": "^8.2",
        "spatie/laravel-sluggable": "^2.4.1",
        "spatie/packagist-api": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6.7",
        "facade/ignition": "^2.0.2",
        "friendsofphp/php-cs-fixer": "^2.16.3",
        "laravel/telescope": "^3.2.1",
        "nunomaduro/collision": "^4.2"
    },
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "BabDev\\": "app/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4.4"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
