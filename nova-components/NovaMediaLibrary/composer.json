{
    "name": "babdev/nova-media-library",
    "description": "A Laravel Nova field integrating basic functionality from Spatie's Media Library.",
    "keywords": [
        "laravel",
        "nova",
        "media-library"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "laravel/nova": "^3.0",
        "spatie/laravel-medialibrary": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "BabDev\\NovaMediaLibrary\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BabDev\\NovaMediaLibrary\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
