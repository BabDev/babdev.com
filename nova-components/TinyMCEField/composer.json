{
    "name": "babdev/tinymce-field",
    "description": "A Laravel Nova field integrating the TinyMCE editor.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "BabDev\\TinyMCEField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BabDev\\TinyMCEField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
